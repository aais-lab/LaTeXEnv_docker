{
	"childfile":{
		"prefix":"childfile",
		"body":[
			"% !TEX root = ${1:root}.tex",
			"$0"
		],
		"description": "ルートファイルを指定すればタイプセットをこのファイルからも行えます"
	},
	"tabref":{
		"prefix":["@tabref","tabref"],
		"body":[
			"表\\ref{$1}$0"
		],
		"description":"表の参照"
	},
	"figref":{
		"prefix":["@figref","figref"],
		"body":[
			"図\\ref{$1}$0"
		],
		"description":"図の参照"
	},
	"cite":{
		"prefix":["@cite","cite"],
		"body":[
			"\\cite{$1}$0"
		],
		"description":"参考文献"
	},
	"input":{
		"prefix":["@input","input"],
		"body":[
			"\\input{$1}$0"
		],
		"description":"ファイル読み込み"
	},
	"ref_chapter":{
		"prefix":["@ref_chap","ref_chap"],
		"body":[
			"\\ref{${1:chap}}章$0"
		],
		"description":"章の参照"
	},
	"ref_section":{
		"prefix":["@ref_sec","ref_sec"],
		"body":[
			"\\ref{${1:sec}}節$0"
		],
		"description":"節の参照"
	},
	"ref_subsection":{
		"prefix":["@ref_sub","ref_sub"],
		"body":[
			"\\ref{${1:sub}}項$0"
		],
		"description":"項の参照"
	},
	"ref_sub2section":{
		"prefix":["@ref_sub2","ref_sub2"],
		"body":[
			"\\ref{${1:sub2}}目$0"
		],
		"description":"目の参照"
	},
	"chapter":{
		"prefix":["@chap","chap"],
		"body":[
			"\\chapter{$1}\\label{chap-$2}$0"
		],
		"description":"章とラベルの作成"
	},
	"section":{
		"prefix":["@sec","sec"],
		"body":[
			"\\section{$1}\\label{sec-$2}$0"
		],
		"description":"節とラベルの作成"
	},
	"subsection":{
		"prefix":["@subsec","subsec"],
		"body":[
			"\\subsection{$1}\\label{sub-$2}$0"
		],
		"description":"項とラベルの作成"
	},
	"sub2section":{
		"prefix":["@sub2sec","sub2sec"],
		"body":[
			"\\subsubsection{$1}\\label{sub2-$2}$0"
		],
		"description":"目とラベルの作成"
	},
	"figure":{
		"prefix":["figin","figureInsert"],
		"body":[
			"\\begin{figure}[h]",
			"\t\\centering",
			"\t\\includegraphics[width=$2cm]{./image/$1}",
			"\t\\caption{$3}",
			"\t\\label{img-$4}",
			"\\end{figure}",
			"",
			"$0"
		],
		"description":"卒論修論の画像挿入"
	},
	"table":{
		"prefix":["tabin","tableInsert"],
		"body":[
			"\\begin{table}[tbh]",
			"\t\\caption{$1}",
			"\t\\label{tab-$2}",
			"\t\\centering",
			"\t\\scalebox{0.9}{",
			"\t\\begin{threeparttable}",
			"\t\t\\begin{tabular}{$3} $4",
			"\t\t$5",
			"\t\t\\end{tabular}",
			"\t\\end{threeparttable}",
			"\t}",
			"\\end{table}",
			""
		],
		"description":"卒論修論の表テンプレ"
	},
	"ref_appendix":{
		"prefix":["@ref_app","ref_app"],
		"body":[
			"付録\\ref{${1:app}}$0"
		],
		"description":"付録の参照"
	}
}
